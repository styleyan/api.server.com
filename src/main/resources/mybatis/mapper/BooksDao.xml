<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.isyxf.blog.dao.BooksDao">
    <insert id="insert">
        INSERT INTO blog_books (`progress`, `book_name`, `book_score`, `book_reason`, `book_evaluate`, `tag_id`)
        VALUES (#{progress}, #{bookName}, #{bookScore}, #{bookReason}, #{bookEvaluate}, #{tagId});
    </insert>

    <delete id="delete" parameterType="int">
      DELETE FROM blog_books WHERE id=#{id};
    </delete>

    <select id="selectAll" resultType="com.isyxf.blog.entity.Books">
      SELECT * FROM blog_books;
    </select>
</mapper>